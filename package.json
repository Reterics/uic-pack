    "email": "attila@reterics.com"
  },
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reterics/uic-pack.git"
  },
  "main": "dist/uic-pack.js",
  "module": "dist/uic-pack.js",
  "types": "dist/uic-pack.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
