    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-storybook": "^0.11.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "storybook": "8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0"
